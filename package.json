{
  "name": "synergysystem-frontend",
  "version": "2.0.0",
  "license": "MIT",
  "main": "server.js",
  "scripts": {
    "start": "ng serve",
    "build": "ng build --build-optimizer --vendor-chunk --aot --output-hashing all --extract-css true --source-map false --named-chunks false",
    "prod": "ng build --prod --build-optimizer --vendor-chunk --aot --output-hashing all --extract-css true --source-map false --named-chunks false",
    "build:prod": "ng build --prod --optimization=false --build-optimizer --vendor-chunk --aot --output-hashing all --extract-css true --source-map false --named-chunks false",
    "test": "ng test",
    "test:coverage": "ng test --codeCoverage",
    "lint": "ng lint",
    "lint-fix": "ng lint synergysystem-frontend --fix",
    "format:fix": "pretty-quick --staged",
    "e2e": "ng e2e"
  },
  "author": "Sebastian Gomez",
  "private": true,
  "dependencies": {
    "@angular/animations": "9.1.4",
    "@angular/cdk": "^9.0.0",
    "@angular/common": "9.1.4",
    "@angular/compiler": "9.1.4",
    "@angular/core": "9.1.4",
    "@angular/flex-layout": "^9.0.0-beta.29",
    "@angular/forms": "9.1.4",
    "@angular/material": "^9.2.2",
    "@angular/platform-browser": "9.1.4",
    "@angular/platform-browser-dynamic": "9.1.4",
    "@angular/router": "9.1.4",
    "@auth0/angular-jwt": "^4.0.0",
    "@sweetalert2/ngx-sweetalert2": "^7.3.0",
    "@sweetalert2/theme-material-ui": "^3.1.0",
    "@swimlane/ngx-datatable": "^16.0.3",
    "@types/socket.io": "^2.1.4",
    "@types/socket.io-client": "^1.4.32",
    "angular-fusioncharts": "^3.0.4",
    "angular2-signaturepad": "^2.11.0",
    "fusioncharts": "^3.15.0-sr.1",
    "husky": "^4.2.5",
    "intl": "^1.2.5",
    "moment": "^2.24.0",
    "ng2-file-upload": "^1.4.0",
    "ng2-validation": "^4.2.0",
    "ngx-export-as": "^1.4.0",
    "ngx-moment": "^3.5.0",
    "ngx-perfect-scrollbar": "^9.0.0",
    "ngx-socket-io": "^3.0.1",
    "npm": "^6.13.7",
    "pretty-quick": "^2.0.1",
    "rxjs": "^6.5.4",
    "saturn-datepicker": "^8.0.5",
    "screenfull": "^5.0.2",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0",
    "sweetalert2": "^9.7.2",
    "terser": "^4.6.3",
    "tslib": "^1.10.0",
    "webpack": "^4.41.5",
    "xlsx": "^0.15.5",
    "zone.js": "^0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.901.4",
    "@angular-devkit/core": "^9.1.4",
    "@angular/cli": "^9.1.4",
    "@angular/compiler-cli": "^9.1.4",
    "@angular/language-service": "9.1.4",
    "@types/jasmine": "^3.5.3",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^13.7.0",
    "codelyzer": "^5.2.1",
    "jasmine-core": "^3.5.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-cli": "^2.0.0",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-jasmine": "^3.1.0",
    "karma-jasmine-html-reporter": "^1.5.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.4",
    "protractor": "^5.4.3",
    "ts-node": "^8.6.2",
    "tslint": "^6.1.1",
    "typescript": "^3.7.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s lint",
      "pre-push": "ng test -- --no-watch"
    }
  }
}
